<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.huangchengxi.handroiduilib">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HAndroidUILib">
        <activity
            android:name=".container.NestedScrollActivity"
            android:exported="true" />
        <activity
            android:name=".container.OverScrollActivity"
            android:exported="true" />
        <activity
            android:name=".system.SystemBottomBarActivity"
            android:exported="true" />
        <activity
            android:name=".buttons.ScrollableButtonActivity"
            android:exported="true" />
        <activity
            android:name=".indicators.PageIndicatorActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>